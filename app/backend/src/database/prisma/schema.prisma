generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int       @id @default(autoincrement())
  name      String
  email     String    @unique
  password  String

  @@map("users")
}

model Product {
  id            Int    @id @default(autoincrement())
  name          String
  description   String
  price         String

  @@map("products")
}